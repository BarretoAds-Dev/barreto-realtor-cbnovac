{
  "name": "barreto-realtor-cbnovac",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "preview": "astro preview",
    "astro": "astro",
    "w-deploy": "wrangler pages deploy",
    "w-dev": "wrangler pages dev",
    "github": "git add . && git commit -m \"Deploy: $(date +'%Y-%m-%d %H:%M:%S')\" && git push origin HEAD",
    "cf-typegen": "wrangler types",
    "s-build": "astro build",
    "clean": "rm -rf dist .astro cache .vite && pnpm store prune",
    "build": "pnpm run clean && astro check && astro build && pnpm run github && pnpm run w-deploy"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.1.0",
    "@astrojs/tailwind": "^5.1.3",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "astro": "^5.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "wrangler": "^3.95.0"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "^12.0.1"
  }
}